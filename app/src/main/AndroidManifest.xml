<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gmail.styles.megan.ChatterBox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="gmail.styles.megan.ChatterBox.MainActivity">
            android:label="@string/app_name"
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="gmail.styles.megan.ChatterBox.CreateEvent"
            android:label="@string/new_event"/>
        <activity android:name="gmail.styles.megan.ChatterBox.OldEvent"
            android:label="@string/old_event"/>
        <activity android:name="gmail.styles.megan.ChatterBox.ActiveEvent"
            android:label="@string/active_event"></activity>
    </application>

</manifest>